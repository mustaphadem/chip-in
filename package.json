{
  "name": "chip-in",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test:api": "cd integrations && npx cypress run --config-file=cypress.config.api.js",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev",
    "db:start": "docker volume create chipin-postgres-data && docker run --rm --name chipin-postgres -e POSTGRES_PASSWORD=postgres -e POSTGRES_DB=chipin_db -p 5432:5432 -v chipin-postgres-data:/var/lib/postgresql/data -d postgres:16",
    "db:stop": "docker stop chipin-postgres"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "client": "^0.0.1",
    "dotenv": "^17.2.3",
    "latest": "^0.2.0",
    "next": "16.0.3",
    "next-auth": "^5.0.0-beta.30",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.3",
    "prisma": "^6.19.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
